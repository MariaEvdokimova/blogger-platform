{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testTimeout": 100000,
  "moduleFileExtensions": ["ts", "js", "json", "node"],
  "transform": {
    "^.+\\.tsx?$": ["ts-jest", {
      "tsconfig": "tsconfig.json"
    }]
  },
  "testMatch": [
    "**/__tests__/**/*.e2e.ts"
  ],
  "verbose": true
}
